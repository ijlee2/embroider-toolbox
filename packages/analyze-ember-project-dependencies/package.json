{
  "name": "analyze-ember-project-dependencies",
  "version": "0.4.5",
  "description": "Analyze dependencies of an Ember project",
  "keywords": [
    "codemod",
    "ember-codemod",
    "emberjs"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/ijlee2/embroider-toolbox.git"
  },
  "license": "MIT",
  "author": "Isaac J. Lee",
  "type": "module",
  "main": "dist/src/index.js",
  "bin": "dist/bin/analyze-ember-project-dependencies.js",
  "directories": {
    "test": "tests"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "./build.sh --production",
    "lint": "concurrently \"pnpm:lint:*(!fix)\" --names \"lint:\"",
    "lint:fix": "concurrently \"pnpm:lint:*:fix\" --names \"fix:\"",
    "lint:js": "eslint . --cache",
    "lint:js:fix": "eslint . --fix",
    "lint:types": "tsc --noEmit",
    "test": "./build.sh --test && mt dist-for-testing --quiet"
  },
  "dependencies": {
    "@codemod-utils/ast-javascript": "^1.2.11",
    "@codemod-utils/ast-template": "^1.1.7",
    "@codemod-utils/ember": "^2.0.3",
    "@codemod-utils/files": "^2.0.7",
    "@codemod-utils/json": "^1.1.12",
    "content-tag": "^3.1.1",
    "yargs": "^17.7.2"
  },
  "devDependencies": {
    "@codemod-utils/tests": "^1.1.10",
    "@ijlee2-frontend-configs/eslint-config-node": "^0.2.1",
    "@ijlee2-frontend-configs/prettier": "^0.2.0",
    "@ijlee2-frontend-configs/typescript": "^0.3.0",
    "@sondr3/minitest": "^0.1.2",
    "@types/node": "^18.19.75",
    "@types/yargs": "^17.0.33",
    "concurrently": "^9.1.2",
    "eslint": "^9.20.0",
    "prettier": "^3.5.0",
    "typescript": "^5.7.3"
  },
  "engines": {
    "node": "18.* || >= 20"
  }
}
